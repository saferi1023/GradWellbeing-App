{
  "expo": {
    "name": "sora",
    "slug": "sora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "sora",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.makex.wellbeing",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationWhenInUseUsageDescription": "This app needs access to location when open for tracking your movements.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location when in the background for tracking significant movements.",
        "NSLocationAlwaysUsageDescription": "This app needs access to location when in the background for tracking significant movements."
      },
      "bundleIdentifier": "com.jsm.sora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.jsm.sora"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for tracking your movements for research purposes."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0d1278f7-476b-4d76-a6a6-61a0ec07addf"
      }
    },
    "owner": "saferi2310"
  }
}
